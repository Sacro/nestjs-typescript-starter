{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via Yarn",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start:dev",
      ],
      "port": 9229
    },
    {
      "type": "cloudcode",
      "language": "Node",
      "request": "attach",
      "debugPort": 9229,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/home/node/app",
      "name": "Attach App on Kubernetes Cluster: NodeJS",
      "podSelector": {
        "app": "nestjs-app"
      }
    },
    {
      "type": "cloudcode",
      "language": "Node",
      "request": "launch",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/home/node/app",
      "protocol": "auto",
      "name": "Launch App on Kubernetes Cluster: NodeJS",
      "podSelector": {
        "app": "nestjs-app",
      }
    }
  ],
  "compounds": []
}
